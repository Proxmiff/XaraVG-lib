<FilterConfig>
  <Filter name="SVGFilter">
    <DisplayName>SVG</DisplayName>
    <Extensions>svg</Extensions>
    <CanImport>svgfilter -c -f %IN%</CanImport>
    <PrepareImport>svgfilterui -i -f %OUT% -x %XML%</PrepareImport>
    <DoImport>svgfilter -i -g -f %IN%</DoImport>
    <PrepareExport>svgfilterui -e -f %OUT% -x %XML%</PrepareExport>
    <DoExport>svgfilter -e -g -f %OUT% -x %XML%</DoExport>
  </Filter>
</FilterConfig>
